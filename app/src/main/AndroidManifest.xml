<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pa5">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Museums"
            android:label="Lists All Museums">
        </activity>

        <activity
            android:name=".LibertyScienceCenter"
            android:label="Liberty Science Center">
        </activity>

        <activity
            android:name=".MontclairArtMuseum"
            android:label="Montclair Art Museum">
        </activity>

        <activity
            android:name=".NewarkMuseumOfArt"
            android:label="Newark Museum Of Art">
        </activity>

        <activity
            android:name=".NJStateMuseum"
            android:label="NJ State Museum">
        </activity>

    </application>
</manifest>